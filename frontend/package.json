{
  "name": "frontend",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "generate:authentication-api": "openapi-generator-cli generate -i src/specs/authentication-specification.yaml -g typescript-angular -o src/app/core/authentication -p=removeOperationIdPrefix=true,modelNameSuffix=Dto",
    "generate:plantation-api": "openapi-generator-cli generate -i src/specs/plantation-specification.yaml -g typescript-angular -o src/app/core/plantations -p=removeOperationIdPrefix=true,modelNameSuffix=Dto",
    "generate:harvests-api": "openapi-generator-cli generate -i src/specs/harvests-specification.yaml -g typescript-angular -o src/app/core/harvests -p=removeOperationIdPrefix=true,modelNameSuffix=Dto",
    "generate:user-stats-api": "openapi-generator-cli generate -i src/specs/user-stats.yaml -g typescript-angular -o src/app/core/user-stats -p=removeOperationIdPrefix=true,modelNameSuffix=Dto"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.0.4",
    "@angular/cdk": "^17.0.1",
    "@angular/common": "^17.0.4",
    "@angular/compiler": "^17.0.4",
    "@angular/core": "^17.0.4",
    "@angular/forms": "^17.0.4",
    "@angular/google-maps": "^17.0.1",
    "@angular/material": "^17.0.1",
    "@angular/platform-browser": "^17.0.4",
    "@angular/platform-browser-dynamic": "^17.0.4",
    "@angular/router": "^17.0.4",
    "@types/googlemaps": "^3.43.3",
    "bootstrap": "^5.3.2",
    "bootstrap-icons": "^1.11.3",
    "ngx-cookie-service": "^17.0.1",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.2",
    "zone.js": "~0.14.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.0.3",
    "@angular/cli": "^17.0.3",
    "@angular/compiler-cli": "^17.0.4",
    "@types/jasmine": "~5.1.4",
    "@openapitools/openapi-generator-cli": "^2.6.0",
    "jasmine-core": "~5.1.1",
    "karma": "~6.4.2",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.1",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "5.2"
  }
}
